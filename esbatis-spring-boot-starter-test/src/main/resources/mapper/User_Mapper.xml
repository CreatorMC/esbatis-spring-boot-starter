<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.creator.esbatis.spring.boot.test.dao.IUserDao">

    <select id="queryUserInfoById" parameterType="java.lang.Long" resultType="com.creator.esbatis.spring.boot.test.entity.Article">
        SELECT *
        FROM article
        where id = #{id}
    </select>

</mapper>
